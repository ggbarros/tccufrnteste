@inproceedings{ WOS:000686550400030,
Author = {Branlard, Emmanuel and Jonkman, Jason and Dana, Scott and Doubrawa,
   Paula},
Book-Group-Author = {IOP},
Title = {A digital twin based on OpenFAST linearizations for real-time load and
   fatigue estimation of land-based turbines},
