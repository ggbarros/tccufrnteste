@inproceedings{ WOS:001304015500006,
Author = {Aghedo, Iwinosa O. and Charlton, Tom S. and Rouainia, Mohamed},
Book-Group-Author = {ASME},
Title = {THE EFFECT OF POST-STORM RECONSOLIDATION ON THE RESPONSE OF MONOPILE-OWT
   SYSTEMS IN CLAY},
Booktitle = {PROCEEDINGS OF ASME 2024 43RD INTERNATIONAL CONFERENCE ON OCEAN,
   OFFSHORE AND ARCTIC ENGINEERING, OMAE2024, VOL 8},
