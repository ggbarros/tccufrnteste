@CONFERENCE{Gaetan2021787,
	author = {Gaetan, Blondet and Sylvain, Girard and Jean-Michel, Heurtier and Thierry, Yalamas},
	title = {A NEW METHODOLOGY FOR FAST LIFESPAN PREDICTION OF OFFSHORE STRUCTURES.},
	year = {2021},
	journal = {Proceedings of the 31st European Safety and Reliability Conference, ESREL 2021},
	pages = {787},
	doi = {10.3850/978-981-18-2016-8_764-cd},
	url = {https://www.scopus.com/inward/record.uri?eid=2-s2.0-85135477608&doi=10.3850%2f978-981-18-2016-8_764-cd&partnerID=40&md5=bb162e09ae542f076007836c6b2995fc},
	affiliations = {PHIMECA ENGINEERING, Centre d’affaires du Zénith, 34 rue de Sarliève, Cournon-Auvergnes, F-63800, France; PRINCIPIA, Zone ATHELIA 1, 215 voie Ariane, LA CIOTAT, 13600, France},
	abstract = {Offshore structures monitoring meets crucial economic and environmental issues. Simulations are performed to estimate the behavior of offshore structures. However, simulations are based on assumptions on operating conditions, which are not always sufficient to take into account all the complexity of the real time-dependent environment and structural properties. Efficient monitoring and maintenance operations relies on data gathered from sensors, which provides real conditions and behavior of the structure. Simulations may be too expensive and time-consuming to estimate the updated lifespan from large dataset of measurements. To address these issues, Phimeca and Principia had developed a methodology based on measurements to obtain rapid and updated prediction of the structure's lifespan. This methodology was successfully applied to the monitoring of a riser of a FPSO (Floating Production Storage and Offloading). First, the episodes of motion measured on the FPSO for 2 years were used to simulate the damage of the riser. The time series describing these episodes were transformed, by applying a combination of machine learning methods, so that the problem dimension was drastically reduced: with initially thousands of variables, only two explain the simulated damage efficiently. Finally, an emulator (metamodel), has been trained and validated to be embedded into the FPSO and to estimate the lifespan of the structure during its life. The dimension reduction makes the emulator numerically light enough to be embedded into the structure and to be compatible with constrained systems. This methodology enables offshore structure owners and operators to obtain a fast updated lifespan prediction from an embedded light software, without any delay due to complex simulations. Thus, decision can be made faster to ensure structural and environmental safety in case of a dangerous event. © ESREL 2021. Published by Research Publishing, Singapore.},
	author_keywords = {digital twin; dimension reduction; lifespan prediction; machine learning; monitoring; Offshore structure},
	keywords = {Digital storage; Large dataset; Machine learning; Offshore oil well production; Offshore oil wells; Offshore structures; Safety engineering; Dimension reduction; Economic issues; Environmental issues; Lifespan prediction; Lifespans; Machine-learning; Operating condition; Real- time; Structure monitoring; Time dependent; Forecasting},
