@CONFERENCE{Knezevic20181786,
	author = {Knezevic, David J. and Kang, HeonYong and Sharma, Partha and Malinowski, Grzegorz and Nguyen, Trong Thuc},
	title = {Structural integrity management of offshore structures via RB-FEA and fast full load mapping based digital twins},
	year = {2018},
	journal = {Proceedings of the International Offshore and Polar Engineering Conference},
	volume = {2018-June},
	pages = {1786 – 1792},
	url = {https://www.scopus.com/inward/record.uri?eid=2-s2.0-85053478525&partnerID=40&md5=853e82e9625a508380ab81eada050953},
	affiliations = {Akselos, Inc., Cambridge, MA, United States; Texas A and M University, College Station, TX, United States; DNV-GL, Katy, TX, United States; Akselos S.A., Lausanne, Switzerland},
	abstract = {In this work we discuss new capabilities for structural integrity analysis of critical offshore assets such as semi-submersibles and FPSOs that are enabled by Reduced Basis FEA (RB-FEA) coupled with Fast Full Load Mapping to enable high-fidelity Digital Twins. RB-FEA is a new structural simulation framework based on component-based model order reduction, in which (i) a large model is decomposed into components, (ii) each component is endowed with parameters which enable geometric, material, and/or load data to be modified on the fly, (iii) a reduced order model of each component (valid for the entire parameter range) is prepared based on an automated pre-analysis of individual components and groups of components, and (iv) component-based models are assembled and solved very efficiently due to re-use of the reduced order model data. For large models this framework enables orders-of-magnitude increase in model detail and solver speed compared to conventional FEA, without sacrificing accuracy. Fast Full Load Mapping obtains full distributive loads acting on a floating body in random waves at costs of conventional global performance analyses, without any costly CFD. In time domain, these full distributive loads are fed into RB-FEA, and the combination of these two powerful approaches enables new capabilities for detailed hydrodynamic and structural analysis of critical offshore assets. We demonstrate these capabilities with some illustrative examples. Copyright © 2018 by the International Society of Offshore and Polar Engineers (ISOPE)},
	author_keywords = {Digital twins; FEA; Full load mapping; Structural analysis},
	keywords = {Arctic engineering; Data reduction; Finite element method; Mapping; Offshore structures; Structural integrity; Time domain analysis; Offshore oil well production; Structural analysis; Component-based modeling; Component-based models; Digital twins; Full-load; Individual components; Reduced order models; Structural integrity analysis; Structural simulations; Structural analysis; Time domain analysis},
	publisher = {International Society of Offshore and Polar Engineers},
	issn = {10986189},
	isbn = {978-188065387-6},
	coden = {POPEE},
	language = {English},
	abbrev_source_title = {Proc Int Offshore Polar Eng Conf},
	type = {Conference paper},
	publication_stage = {Final},
	source = {Scopus},
	note = {Cited by: 6; Conference name: 28th International Ocean and Polar Engineering Conference, ISOPE 2018; Conference date: 10 June 2018 through 15 June 2018; Conference code: 138813}
}

@CONFERENCE{Sharma20182942,
	author = {Sharma, Partha and Knezevic, David and Huynh, Phuong and Malinowski, Grzegorz},
	title = {RB-FEA based digital twin for structural integrity assessment of Offshore structures},
	year = {2018},
	journal = {Proceedings of the Annual Offshore Technology Conference},
	volume = {4},
	pages = {2942 – 2947},
	doi = {10.4043/29005-ms},
	url = {https://www.scopus.com/inward/record.uri?eid=2-s2.0-85050542260&doi=10.4043%2f29005-ms&partnerID=40&md5=aea16e6c3fa52f88ba57125fc91aec2a},
	affiliations = {DNV GL, Katy, United States; Akselos, Cambridge, United Kingdom},
	abstract = {The reduced basis finite element analysis (RB-FEA) technology was utilized for performing structural analyses of a semisubmersible drilling rig. The digital twin of the drilling rig based on the RB-FEA technology is about 1000 times faster to analyze than a conventional FEA model, without any compromise in the accuracy of the solution. The global model was established by integrating the individual components of the RB-FEA model with associated material properties, loads, and boundary conditions. The software for mapping of the wave loads (generated by software such as WAMIT/WADAM) onto the hull structure of the drilling rig was developed. Time domain strength and fatigue analyses were then performed for the whole structure that also included the highly refined fatigue sensitive regions of the drilling rig. The results from the study demonstrated the efficiency and the accuracy of the RB-FEA based technology. The speedup and level of detail achieved through the RB-FEA technology is a key enabler for the digital twin technology since it enables true condition-based modeling of large and critical assets, allowing all relevant structural details to be included in the model, along with sensor and inspection-based condition data such as cracks, corrosion, and damage due to impact or collision. Asset life extension - enabled by the digital twin and condition-based monitoring using sensor systems - is the key to capitalizing on the value of existing facilities that are operating near (or beyond) their original design life. © 2018, Offshore Technology Conference.},
	keywords = {Corrosion; Digital twin; Drilling platforms; Drilling rigs; Fatigue of materials; Finite element method; Infill drilling; Offshore oil well production; Time domain analysis; Asset life extensions; Condition-based monitoring; Fatigue analysis; Individual components; Semi-submersible drilling; Sensitive regions; Structural details; Structural integrity assessment; Offshore technology},
