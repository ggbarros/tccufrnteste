@CONFERENCE{Cai2021,
	author = {Cai, Hui and Song, Xinya and Zeng, Yuelin and Jiang, Teng and Schlegel, Steffen and Westermann, Dirk},
	title = {A practical approach to construct a digital twin of a power grid using harmonic spectra},
	year = {2021},
