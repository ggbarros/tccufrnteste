@ARTICLE{Liu2022126,
	author = {Liu, Le and Lekić, Aleksandra and Popov, Marjan},
	title = {Robust Adaptive Back-Stepping Control Approach Using Quadratic Lyapunov Functions for MMC-Based HVDC Digital Twins},
