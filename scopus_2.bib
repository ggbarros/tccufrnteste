	keywords = {Electric power transmission networks; Electric utilities; Fossil fuels; Harmonic analysis; Observability; Offshore oil well production; Offshore wind farms; State estimation; Electrical energy systems; Harmonic spectrum; Hydropower plants; Network state; Network state monitoring; PhotoVoltaic plant; Power grids; Renewable energies; State monitoring; Sustainable energy supply; Neural networks},
