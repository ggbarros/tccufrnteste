@CONFERENCE{Yiew2020,
	author = {Yiew, Lucas J. and Jin, Yuting and Zheng, Yingying and Magee, Allan R.},
	title = {Thruster performance of an azimuth stern drive tug},
	year = {2020},
	journal = {Proceedings of the International Conference on Offshore Mechanics and Arctic Engineering - OMAE},
	volume = {6B-2020},
	url = {https://www.scopus.com/inward/record.uri?eid=2-s2.0-85099408032&partnerID=40&md5=4c602234290bc9e79aa59065c67997b3},
	affiliations = {Technology Centre for Offshore and Marine, Singapore (TCOMS), Singapore, 118411, Singapore},
	abstract = {The development of an accurate digital performance twin of a tug requires a complete understanding of its propulsive capacity and hull-thruster interactions. In this study, the propulsion characteristics of an Azimuth Stern Drive (ASD) tug is investigated using model-scale Reynolds-averaged Navier-Stokes (RANS) simulations. The propulsion plant consists of two counter-rotating thruster units, with each having a Ka4-70 series propeller and 19A duct profile. Comparisons in propulsive performances using the steady-state moving reference frame (MRF) approach and the transient rigid body motion (RBM) models are shown, and validated against data from openwater experiments. The MRF method gives sufficiently accurate predictions of thrust and torque in forward flow and moderate angles-of-attack, while the RBM method performs better at larger inflow angles. The effects of thruster-hull and thruster-thruster interactions on wake characteristics and propulsion performance are also investigated over a range of advance and inflow/azimuth angles. Convergence and mesh independence studies are conducted to determine the optimal spatial and temporal simulation parameters. Results from this study identify flow regimes where hull and thruster interactions are significant. © 2020 American Society of Mechanical Engineers (ASME). All rights reserved.},
	keywords = {Angle of attack; Digital storage; Magnetorheological fluids; Navier Stokes equations; Ocean engineering; Offshore oil well production; Propulsion; Structural frames; Accurate prediction; Moving reference frame; Propulsion characteristics; Propulsion performance; Propulsive performance; Reynolds-averaged navier-stokes simulations; Thruster interaction; Thruster performance; Arctic engineering},
