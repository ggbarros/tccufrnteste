@ARTICLE{Myers201758,
	author = {Myers, Greg},
	title = {Digital twin for marine drilling risers},
	year = {2017},
	journal = {Offshore Engineer},
	volume = {42},
	number = {5},
	pages = {58 – 63},
	url = {https://www.scopus.com/inward/record.uri?eid=2-s2.0-85018324202&partnerID=40&md5=6e9393a424145a77e1d380546f82b355},
	affiliations = {Subsea and Drilling Unit of GE Oil and Gas, United States},
	abstract = {Digital transformation of offshore assets enables a competitive advantage by reducing excessive inspection and maintenance costs and increasing uptime with real time operational data. GE Oil & Gas has addressed these challenges by creating a digital model of a physical asset, called a digital twin, which can then be optimized to drive enhanced business outcomes. In this instance, both a physical and a virtual model of a marine driller riser was created with the goal of reducing downtime or optimized inspection schedules of these critical assets, which can extend miles in length below the ocean's surface. The digital twin allows GE Oil & Gas to provide the driller with data-based diagnostics and insights into what is happening to the asset during a regular day or even during an extreme event, and then pick the most optimal way to run that asset. This marine riser digital twin will allow operators to maintain equipment in a more efficient manner. It can help reduce excessive maintenance and costs of the main tube for drilling contractors with a data-driven approach. The digital twin, or machine learning model, is a virtual model of the riser that is continuously updated against the sensor measurements and metocean conditions. When this data is beyond the original training conditions, a model retraining is automatically triggered. Visualization and alerts are provided by software, which ingests and performs advanced analytics on the field data enhance the operational decision-making for the drillers and operators. This continuous learning and update of the digital twin model allows us to provide more precise calculation of the riser fatigue life and enable optimizing operations in near real-time.},
