	note = {Cited by: 7; Conference name: Offshore Technology Conference Asia 2020, OTCA 2020; Conference date: 2 November 2020 through 6 November 2020; Conference code: 165202}
}

@CONFERENCE{Podskarbi2020,
	author = {Podskarbi, Mat and Knezevic, David J},
	title = {Digital twin for operations-present applications and future digital thread},
	year = {2020},
