	publisher = {Institute of Physics},
	issn = {17426588},
	language = {English},
	abbrev_source_title = {J. Phys. Conf. Ser.},
	type = {Conference paper},
	publication_stage = {Final},
	source = {Scopus},
	note = {Cited by: 1; Conference name: International Maritime and Port Technology and Development Conference, MTEC 2022 and 4th International Conference on Maritime Autonomous Surface Ships, ICMASS 2022; Conference date: 5 April 2022 through 7 April 2022; Conference code: 181745; All Open Access, Gold Open Access}
}

