@CONFERENCE{Oliveira2023,
	author = {Oliveira, L.A. and da Silva, F.R.G. and Bellumat, E. and Gama, W.C. and Costa, M.V.F. and Donatti, C.N. and Rosenbach, L.I.N.C. and Fasolin, K. and Martins, A.L.},
