	source = {Scopus},
	note = {Cited by: 3; Conference name: Offshore Technology Conference 2020, OTC 2020; Conference date: 4 May 2020 through 7 May 2020; Conference code: 160167}
}

@CONFERENCE{Samnejad2020,
