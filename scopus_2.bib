	note = {Cited by: 1; Conference name: Offshore Technology Conference, OTC 2022; Conference date: 2 May 2022 through 5 May 2022; Conference code: 290449}
}

@CONFERENCE{Matsumoto2022,
	author = {Matsumoto, Shunsaku and Jaiswal, Vivek and Sugimura, Tadashi and Yoshida, Kohei and Ishii, Hidekazu and Tsukahara, Keisuke and Honjo, Shintaro and Szalewski, Piotr and Kumar, Praveen and Rajput, Amit},
