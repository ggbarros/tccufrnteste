@ARTICLE{Andersen2024,
	author = {Andersen, Martin Lieberkind and Sævik, Svein and Wu, Jie and Leira, Bernt Johan and Langseth, Helge},
	title = {Applying Bayesian optimization to predict parameters in a time-domain model for cross-flow vortex-induced vibrations},
	year = {2024},
	journal = {Marine Structures},
	volume = {94},
	doi = {10.1016/j.marstruc.2023.103571},
	url = {https://www.scopus.com/inward/record.uri?eid=2-s2.0-85181770073&doi=10.1016%2fj.marstruc.2023.103571&partnerID=40&md5=3337555f1e6eae385327d57619f8e716},
	affiliations = {Department of Marine Technology, NTNU, Jonsvannsveien 82, Trondheim, 7050, Norway; SINTEF Ocean, Jonsvannsveien 82, Trondheim, 7050, Norway; Department of Computer Science, NTNU, Høgskoleringen 1, Trondheim, 7034, Norway},
	abstract = {As the demand for harvesting offshore energy increases worldwide, the need for slender structures, such as marine risers and power cables, will increase. The dominating loads for deep water applications will primarily be caused by current-structure interactions, where vortex-induced vibrations (VIV) are known to be a challenging response type to handle correctly during design. Semi-empirical time-domain models are promising for VIV prediction, but uncertainties related to simplifications in the hydrodynamic load model and empirical parameters lead to over-conservative designs, where the parameters are estimated from model tests within a relatively low Reynolds number range. The aim of this paper is to present an efficient tool to estimate empirical hydrodynamic parameters directly from test data. To illustrate the method, the hydrodynamic parameters were estimated from pure cross-flow VIV model tests in a steady current with Reynolds numbers in the range of 40,000-120,000. The parameters were updated sequentially by using a Bayesian optimization framework, with the aim of minimizing an objective function that incorporates the response errors between time-domain VIV simulations and model test data. It is shown that three empirical parameters can be obtained simultaneously, resulting in small response errors but the optimal parameters were overly sensitive to variations in the flow velocity. The optimal parameters were used to reconstruct the drag amplification related to cross-flow VIV and in forced motion simulations to illustrate how the parameters in the load model would map into the traditional formulation in terms of the added mass and excitation coefficient. © 2023},
	author_keywords = {Bayesian optimization; Digital twins; Drag amplification; Semi-empirical models; Vortex-induced vibrations},
	keywords = {Drag; Flow velocity; Fluid structure interaction; Marine risers; Offshore oil well production; Parameter estimation; Reynolds number; Structural design; Time domain analysis; Uncertainty analysis; Vibrations (mechanical); Vortex flow; Bayesian optimization; Crossflow vortices; Drag amplification; Empirical parameters; Hydrodynamic parameters; Load modeling; Model tests; Semiempirical models; Time domain models; Vortex induced vibration; Bayesian analysis; cable; computer simulation; design; energy resource; error analysis; flow field; flow velocity; hydrodynamics; loading test; model test; numerical model; offshore engineering; optimization; parameterization; prediction; Reynolds number; riser; vibration; vortex flow; Hydrodynamics},
	correspondence_address = {M.L. Andersen; Department of Marine Technology, NTNU, Trondheim, Jonsvannsveien 82, 7050, Norway; email: martin.andersen@ntnu.no},
	publisher = {Elsevier Ltd},
