	keywords = {Condition monitoring; Data Analytics; Machine learning; Neural networks; Offshore wind turbines; Turbomachine blades; Wind turbine blades; 'current; Assets management; Corrosion-fatigue; Digital tools; Fatigue life assessment; Framework development; Horizontal axis; Machine-learning; Monopiles; Offshore winds; Offshore oil well production},
	correspondence_address = {M. Bodaghi; Department of Engineering, School of Science and Technology, Nottingham Trent University, Nottingham, NG11 8NS, United Kingdom; email: mahdi.bodaghi@ntu.ac.uk},
