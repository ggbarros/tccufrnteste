	year = {2023},
	journal = {Renewable Energy Focus },
	volume = {44},
	pages = {390 – 412},
	doi = {10.1016/j.ref.2022.08.005},
	url = {https://www.scopus.com/inward/record.uri?eid=2-s2.0-85138164895&doi=10.1016%2fj.ref.2022.08.005&partnerID=40&md5=79e1934e39d8f4cce79ae926dc40baa4},
	affiliations = {Bristol Composites Institute, Department of Aerospace Engineering, University of Bristol, Queen's Building, University Walk, Bristol, BS8 1TR, United Kingdom; Offshore Renewable Energy (ORE) Catapult, Inovo Building, 121 George Street, Glasgow, United Kingdom},
	abstract = {In recent decades, renewable energy has attracted attention as a viable energy supply. Among renewable energy sources, offshore wind energy has been considerably growing since longer and larger wind turbine composite blades were deployed. The manufacture of the longer and larger composite blades leads to more wind energy production. However, the wind turbine composite blades are susceptible to damage and defects due to multiple structural loads and harsh operating environments in service. Hence, condition monitoring and maintenance of wind turbine composite blades require in-depth investigation to prevent structural damage and defects and to improve remaining lifetime of the composite structure. The types of damage and defects in wind turbine composite blades are typically delamination, debonding, and cracks, which are influenced by the intrinsic structural nonlinearities, manufacturing process stage, and harsh environmental impacts in service. For these reasons, the regular condition monitoring of the composite blades is required to assess degradation in performance and structural condition to minimise levelised energy costs for maintenance. To improve reliability and sustainability, data-driven inspection with digital twin technology is reviewed as a trend of condition monitoring frameworks. Advanced functional materials to potentially assist current non-destructive testing (NDT) methods or to be utilised as self-sensing performance are reviewed. From manufacturing to the system level, a comprehensive review on progress and trends of monitoring of wind turbine composite blades is carried out including physics-based NDT methods, data fusion in sensor networks, automated system, mechanics, and digital twin technology with the environmental coupling. © 2022 The Authors},
	author_keywords = {Condition monitoring; Digital twin; Maintenance; Non-destructive testing; Wind turbine blades},
	keywords = {Automation; Condition based maintenance; Damage detection; Environmental impact; Environmental technology; Functional materials; Offshore oil well production; Structural health monitoring; Sustainable development; Turbine components; Turbomachine blades; Wind power; Wind turbine blades; Composite blades; Data driven; Detection methods; Energy supplies; Non destructive testing; Nondestructive testing method; Off-shore wind energy; Renewable energies; Renewable energy source; Wind turbine blades; Nondestructive examination},
	correspondence_address = {K. Kong; Bristol Composites Institute, Department of Aerospace Engineering, University of Bristol, Queen's Building, University Walk, Bristol, BS8 1TR, United Kingdom; email: kyungil.kong@bristol.ac.uk},
	publisher = {Elsevier Ltd},
	issn = {17550084},
	language = {English},
	abbrev_source_title = {Renew. Energy Focus},
	type = {Review},
	publication_stage = {Final},
	source = {Scopus},
	note = {Cited by: 30; All Open Access, Hybrid Gold Open Access}
}

@ARTICLE{Kim2019,
	author = {Kim, Hyoung-Chul and Kim, Moo-Hyun and Choe, Do-Eun},
	title = {Structural health monitoring of towers and blades for floating offshore wind turbines using operational modal analysis and modal properties with numerical-sensor signals},
	year = {2019},
	journal = {Ocean Engineering},
	volume = {188},
	doi = {10.1016/j.oceaneng.2019.106226},
	url = {https://www.scopus.com/inward/record.uri?eid=2-s2.0-85069968676&doi=10.1016%2fj.oceaneng.2019.106226&partnerID=40&md5=38261f83d22d6b3405a8c16adf3d0889},
	affiliations = {Ocean Engineering Department, Texas A&M University, College Station, 77843, TX, United States; Civil and Environmental Engineering Department, Prairie View A&M University, Prairie View, 77446, TX, United States},
	abstract = {In the present study, a structural health monitoring (SHM) method for floating offshore wind turbines (FOWTs) is suggested and tested using operational modal analysis (OMA) with numerical-sensor signals. The numerical accelerometer signals along the tower and blade of FOWT in dynamic wind field are used for the OMA. The numerical-sensor signals are simulated using a time-domain turbine-floater-mooring fully-coupled dynamic simulation computer program. To perform the SHM of a FOWT through OMA, natural frequencies, displacement mode shapes (DMS), and curvature mode shapes (CMS) of the tower and blades are obtained and analyzed. The modal properties are systematically compared between the intact and damaged conditions. Their differences are used for damage detection. The results show that CMS is found to be the most effective modal property to detect damage locations and intensities. The performance of the damage detection based on the OMA-CMS analysis is verified by independent FEM (finite element method) results. This study is unique in that the SHM using OMA and CMS for FOWTs is implemented including the floater-tower-blade coupling effects. The technology can contribute to the design of remote SHM system for future FOWTs. A further validated model with field data may build up a huge database for various damage scenarios so that it can be applied to digital-twin-based smart health monitoring technology. © 2019},
	author_keywords = {Curvature mode shape; Damage detection; Floating offshore wind turbine; Health monitoring; Numerical sensor; Operational modal analysis; Turbine-floater-mooring coupled dynamics simulation},
	keywords = {Buoys; Damage detection; Modal analysis; Mooring; Numerical methods; Offshore oil well production; Structural health monitoring; Time domain analysis; Towers; Turbomachine blades; Coupled dynamics; Floating offshore wind turbines; Health monitoring; Mode shapes; Operational modal analysis; floating offshore structure; health monitoring; mooring system; sensor; wind field; wind turbine; Offshore wind turbines},
	correspondence_address = {H.-C. Kim; Ocean Engineering Department, Texas A&M University, College Station, 77843, United States; email: doriduul@tamu.edu},
	publisher = {Elsevier Ltd},
