@ARTICLE{Zhao2023531,
	author = {Zhao, Xiang and Dao, My Ha and Le, Quang Tuyen},
	title = {Digital twining of an offshore wind turbine on a monopile using reduced-order modelling approach},
	year = {2023},
	journal = {Renewable Energy},
	volume = {206},
	pages = {531 – 551},
	doi = {10.1016/j.renene.2023.02.067},
	url = {https://www.scopus.com/inward/record.uri?eid=2-s2.0-85149849769&doi=10.1016%2fj.renene.2023.02.067&partnerID=40&md5=e1ef60d39b36ee4482b83557fc490ef0},
	affiliations = {Institute of High Performance Computing (IHPC), Agency for Science, Technology and Research (A*STAR), 1 Fusionopolis Way, #16-16 Connexis, Singapore, 138632, Singapore},
	abstract = {In the wind energy industry, a cost-benefit digital twin (DT) will be very useful for managing the operation of a wind turbine. For instance, a DT could provide information on structural health conditions in real-time as well as projections for the near future. In this work, we employ a component-based Reduced-Order Modelling (ROM) technique to construct a DT of a parameter-varying offshore wind turbine system on a monopile. The DT consists of a ROM model for modal analysis and structural response prediction under wind and wave loadings. First, an offline library is pre-computed, which contains a series of component archetypes. Then, the component-based ROM is assembled out of these archetypes, such as the parts in the blade, hub, nacelle, and tower. With the computation speed of two orders (approximately 650x) faster than a Finite Element Analysis (FEA) model and high accuracy (less than 0.2% error), the DT could be able to provide almost instant predictions of the modes and the responses of the turbine structure due to the wind-wave loading as well as projections of structural health conditions. © 2023 Elsevier Ltd},
	author_keywords = {Computational fluid dynamics; Digital twin; Elasticity; Reduced order model; Structural response; Wind turbine},
	keywords = {Cost benefit analysis; Modal analysis; Offshore oil well production; Offshore wind turbines; Wind power; Component based; Health condition; Modeling approach; Monopiles; Reduced order modelling; Reduced-order model; Structural health; Structural response; Wave loadings; Wind energy industry; computational fluid dynamics; finite element method; modeling; operations technology; wind power; wind turbine; Computational fluid dynamics},
	correspondence_address = {X. Zhao; Institute of High Performance Computing (IHPC), Agency for Science, Technology and Research (A*STAR), 1 Fusionopolis Way, #16-16 Connexis, Singapore, 138632, Singapore; email: xiang_zhao@u.nus.edu},
	publisher = {Elsevier Ltd},
	issn = {09601481},
	language = {English},
	abbrev_source_title = {Renew. Energy},
	type = {Article},
	publication_stage = {Final},
