@ARTICLE{Wang2024,
	author = {Wang, Rui and Huang, Weiguo and Lu, Yixiang and Wang, Jun and Ding, Chuancang and Liao, Yi and Shi, Juanjuan},
	title = {Cloud-edge collaborative transfer fault diagnosis of rotating machinery via federated fine-tuning and target self-adaptation},
	year = {2024},
	journal = {Expert Systems with Applications},
	volume = {250},
	doi = {10.1016/j.eswa.2024.123859},
	url = {https://www.scopus.com/inward/record.uri?eid=2-s2.0-85189749862&doi=10.1016%2fj.eswa.2024.123859&partnerID=40&md5=db6e67bab749f5a0fdb281a868f2eb62},
	affiliations = {School of Rail Transportation, Soochow University, Suzhou, 215131, China; Intelligent Urban Rail Engineering Research Center of Jiangsu Province, Soochow University, Suzhou, 215131, China; Anhui Engineering Laboratory of Human Robot Integration System Equipment, Anhui University, Hefei, 230601, China},
	abstract = {The data-driven fault diagnostic methods have made significant advances and breakthroughs in the past decades. However, due to the huge time and labor costs, single user is hard to collect sufficient monitoring data for model training and multiple users are preferred for collaborative modeling with distributed data. Considering the concerns of data privacy or conflicts of interest, the local data of different users cannot be directly shared with others for centralized training. Moreover, they are usually sampled from different working environments, leading to data island and distribution shift issues. To solve the two issues, this paper proposes a novel cloud-edge collaborative transfer fault diagnosis approach for rotating machinery. The federated learning scheme is introduced for collaborative modeling of multiple users while ensuring the data privacy of participants. To bridge the distribution gap across clients, the source training module is constructed to conduct local model training on insufficient labeled data from different users, and a federated fine-tuning module is then developed to remove distribution discrepancies between different source users, thus building accurate source models. Finally, a target self-adaptation module is designed to reduce prediction differences of source client models on target samples and achieve effective adaptation of the target client to source clients. Comprehensive comparative analysis shows that the proposed approach achieves a performance gain of 0.45% to the traditional centralized transfer learning method, demonstrating its potential in addressing the data island and distribution shift issues in practical applications. © 2024 Elsevier Ltd},
	author_keywords = {Collaborative fault diagnosis; Data privacy; Federated learning; Rotating machinery; Transfer learning},
	keywords = {Failure analysis; Fault detection; Learning systems; Rotating machinery; Wages; Collaborative fault diagnose; Collaborative modeling; Faults diagnosis; Federated learning; Fine tuning; Model training; Multiple user; Self- adaptations; Transfer faults; Transfer learning; Data privacy},
	correspondence_address = {W. Huang; School of Rail Transportation, Soochow University, Suzhou, 215131, China; email: wghuang@suda.edu.cn},
	publisher = {Elsevier Ltd},
	issn = {09574174},
