@article{ WOS:000698893600001,
Author = {Walker, Jake and Coraddu, Andrea and Collu, Maurizio and Oneto, Luca},
Title = {Digital twins of the mooring line tension for floating offshore wind
   turbines to improve monitoring, lifespan, and safety},
