	keywords = {Aerial photography; Animals; Cameras; Conservation; Ecology; Electric utilities; Environmental impact; Environmental impact assessments; Flight dynamics; Image processing; Mammals; Motion analysis; Motion compensation; Quality assurance; Remote sensing; Surveys; Tracking (position); Wind power; Wind turbines; Biological monitoring; Development and testing; Digital surveys; Environmental assessment; Ground sampling distances; Marine wildlife; Offshore winds; Regulatory requirements; Offshore wind farms},
	editor = {Zdimal V. and Ramasamy S.M. and Skidmore A. and Altan O. and Comiso J. and Thenkabail P.S. and Halounova L. and Safar V. and Planka L. and Raju P.L.N. and Srinivasa Kumar T. and Lavender S. and (Yousif) Hussin Y.A. and Kerr Y. and Yue P. and Patias P. and Faruque F.S. and Weng Q.},
