	editor = {Wang X. and Pegg N.},
	publisher = {Society of Naval Architects and Marine Engineers},
	isbn = {979-835070678-9},
	language = {English},
	abbrev_source_title = {Proc. Int. Ship Offshore Struct. Congr., ISSC},
	type = {Conference paper},
	publication_stage = {Final},
	source = {Scopus},
	note = {Cited by: 1; Conference name: 21st International Ship and Offshore Structures Congress, ISSC 2022; Conference date: 11 September 2022 through 15 September 2022; Conference code: 187995; All Open Access, Bronze Open Access}
}

