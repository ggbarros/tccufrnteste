@CONFERENCE{Knezevic2022,
	author = {Knezevic, David and Pinguet, Romain and Martin, Bruce and Peterson, John and Podskarbi, Mat and Price, Seth and Vallaghe, Sylvain},
	title = {VALIDATION OF A FULLY INTEGRATED NUMERICAL SIMULATION METHOD TO ASSESS STRUCTURAL INTEGRITY ON A FLEXIBLE MODEL OF FOWT USING RB-FEA},
	year = {2022},
	journal = {Proceedings of the ASME 2022 4th International Offshore Wind Technical Conference, IOWTC2022},
	doi = {10.1115/IOWTC2022-94295},
	url = {https://www.scopus.com/inward/record.uri?eid=2-s2.0-85148056000&doi=10.1115%2fIOWTC2022-94295&partnerID=40&md5=807503cb613d5e4f1f4c99aa8829debb},
	affiliations = {Akselos Inc., Boston, MA, United States; Principle Power, Emeryville, CA, United States},
	abstract = {This work is part of the DIGIFLOAT project. This project aims to provide a digital twin software framework that enables high-fidelity structural integrity analysis for a floating offshore wind turbine (FOWT) in its as-is state – we refer to this as a structural digital twin (SDT). The same framework is also applicable at design time to assess an extensive range of operational conditions with high-fidelity analysis. In order to achieve this purpose, we validate in this work, a complete system-level FOWT structural analysis framework, which is integrated with NREL OpenFAST and Akselos’ Integra® software. OpenFAST provides the global motions and loads, and these are mapped in the time-domain to the structural model for structural analysis. The structural analysis includes all relevant loads from the system-level model: wind turbine aerodynamics, hydrodynamics, servo-control, and mooring. We demonstrate that the Reduced Basis Finite Element Analysis (RB-FEA) used in Integra® makes detailed structural analysis of FOWT hulls efficient, and that this is an enabler for FOWT SDTs. Copyright © 2022 by ASME.},
	author_keywords = {DeepCwind; FEA; FOWT; Load Mapping; RB-FEA; Structural Analysis},
	keywords = {Aerodynamics; Computer programming; Finite element method; Numerical methods; Offshore oil well production; Time domain analysis; Deepcwind; Finite element analyse; Flexible model; Floating offshore wind turbines; Fully integrated; Load mapping; Numerical simulation method; Reduced base finite element analyse; Reduced basis; Software frameworks; Structural analysis},
	publisher = {American Society of Mechanical Engineers (ASME)},
	isbn = {978-079188661-8},
	language = {English},
	abbrev_source_title = {Proc. ASME Int. Offshore Wind Tech. Conf., IOWTC},
	type = {Conference paper},
