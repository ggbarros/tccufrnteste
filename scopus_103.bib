	doi = {10.1115/OMAE2023-104715},
	url = {https://www.scopus.com/inward/record.uri?eid=2-s2.0-85173618814&doi=10.1115%2fOMAE2023-104715&partnerID=40&md5=9deb1292353ce9e410c05105215120a0},
